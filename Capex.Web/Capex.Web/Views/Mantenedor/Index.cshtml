    <script>
            FnSeleccionarTipoIniciativa = function (tipo) {
                console.log("FnSeleccionarTipoIniciativa tipo=", tipo);
                if (tipo == '-1') {
                    $('#idBotonTipoIniciativa').prop('disabled', true);
                } else {
                    $('#idBotonTipoIniciativa').prop('disabled', false);
                }
            }

            FnSeleccionarTipoIniciativaBloqueo = function (tipo) {
                console.log("FnSeleccionarTipoIniciativaBloqueo tipo=", tipo);
                if (tipo == '-1') {
                    $('#idBotonTipoIniciativaBloqueo').prop('disabled', true);
                } else {
                    $('#idBotonTipoIniciativaBloqueo').prop('disabled', false);
                }
            }

            FnProcesarSeleccionTipoIniciativa = function () {
                var url = '@Url.Action("TemplateIniciativa", "Mantenedor", new { tipoIniciativaSeleccionado = "__id__" })';
                var param = $('#idTipoIniciativa').val();
                window.location.href = "Mantenedor/TemplateIniciativa?tipoIniciativaSeleccionado=" + param; //url.replace('__id__', param);
            }

            FnProcesarSeleccionTipoIniciativaBloqueo = function () {
                var url = '@Url.Action("Bloqueo", "Mantenedor", new { tipoIniciativaSeleccionado = "__id__" })';
                var param = $('#idTipoIniciativaBloqueo').val();
                window.location.href = "Mantenedor/Bloqueo?tipoIniciativaSeleccionado=" + param; //url.replace('__id__', param);
            }

            $(document).ready(function () {
                $("#ModalTipoIniciativa").on('show.bs.modal', function () {
                    $('#idTipoIniciativa').val('-1');
                    $('#idBotonTipoIniciativa').prop('disabled', true);
                });

                $("#ModalTipoIniciativaBloqueo").on('show.bs.modal', function () {
                    $('#idTipoIniciativaBloqueo').val('-1');
                    $('#idBotonTipoIniciativaBloqueo').prop('disabled', true);
                });
            });
    </script>

