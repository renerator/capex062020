                    $(selectorActualizarAnioHasta).val(ultimoAnio);
                }
            });
            $(selectorActualizarAnioHasta).on('change', function () {
                console.log('Change event.');
                var val = $(selectorActualizarAnioHasta).val();
                console.log('Change event. val=', val);
            });
            $(selectorActualizarAnioHasta).change(function () {
                console.log('Second change event...');
            });
            $(selectorActualizarAnioHasta).number(true, 0, ',', '');

            $("input[name=optradio]").change(function () {
                console.log("input[name=optradio]=", $(this).val());
                window.location.href = location.protocol + '//' + location.host + location.pathname + "?tipoIniciativaSeleccionado=" + $(this).val();
            });

            $('#TemplateAnio').on('change', function () {
                console.log("TemplateAnio this.value=", this.value);
                var anioSeleccionado = this.value;
                if (anioSeleccionado == "-1") {
                    $('#guadarTemplate').prop('disabled', true);
                    $('#aplicarLongTime').prop('disabled', true);
                    $('#anioHasta').val("0");
                    $('#anioDesde').val("0");
                    $('#anioHasta').prop('disabled', true);
                    $('#anioDesde').prop('disabled', true);
                    var paso;
                    for (paso = 1; paso <= pasoFinal; paso++) {
                        var selectorAnio = '#AnioMas' + paso;
                        $(selectorAnio).html('&nbsp;&nbsp;...&nbsp;');
                    };
                } else {
                    var paso;
                    for (paso = 1; paso <= pasoFinal; paso++) {
                        var anio = '<strong>' + (parseInt(anioSeleccionado) + paso) + '</strong>';
                        var selectorAnio = '#AnioMas' + paso;
                        $(selectorAnio).html(anio);
                    };
                    $('#guadarTemplate').prop('disabled', false);
                    $('#aplicarLongTime').prop('disabled', false);
                    $('#anioHasta').prop('disabled', false);
                    $('#anioDesde').prop('disabled', false);
                }
            });

            $('#ActualizarTemplateAnio').on('change', function () {
                console.log("TemplateAnioActualizar this.value=", this.value);
                FNActualizarAnioChange(this.value);
            });
        });
    </script>
</body>
</html>