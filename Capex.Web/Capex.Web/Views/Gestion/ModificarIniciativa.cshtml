                                                                        <label for="Proceso">Proceso</label>
                                                                        <select class="form-control" style="height:36px; font-size:11px;" id="Proceso" name="Proceso" onchange="FNGuardarSeleccion('COMBO_PROCESO',this.value)">
                                                                            <option selected>Seleccionar..</option>
                                                                        </select>
                                                                    </div>
