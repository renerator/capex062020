                                                                    </div>
                                                                    <div class="col-sm-2 pb-1">
                                                                        <label for="Area">Area</label>
                                                                        <select class="form-control" style="height:36px; font-size:11px;" id="Area" name="Area" onchange="FNGuardarSeleccion('COMBO_AREA',this.value)">
                                                                            <option selected>Seleccionar..</option>
                                                                        </select>
                                                                    </div>
                                                                    <div class="col-sm-2 pb-1">
                                                                        <label for="Compania">Compañia</label>
                                                                        <select class="form-control" style="height:36px; font-size:11px;" id="Compania" name="Compania" onchange="FNGuardarSeleccion('COMBO_COMPANIA', this.value); FNObtenerTokenCompania(this.value);">
                                                                            <option selected>Seleccionar..</option>
                                                                        </select>
                                                                    </div>
                                                                    <div class="col-sm-2 pb-1">
                                                                        <label for="Etapa">Etapa</label>
                                                                        <select class="form-control" style="height:36px; font-size:11px;" id="Etapa" name="Etapa" onchange="FNGuardarSeleccion('COMBO_ETAPA',this.value)">
                                                                            <option selected>Seleccionar..</option>
                                                                        </select>
                                                                    </div>
                                                                    <div class="col-sm-4 pb-1">
                                                                        <label for="NombreProyecto">Nombre del Proyecto</label>
                                                                        <input type="text" class="form-control" style="height:36px; font-size:11px;text-transform:uppercase; background-color:#517c89;color:#f0f0f0;" value="@Html.Raw(@ViewBag.IdenNombre)" id="NombreProyecto" name="NombreProyecto" disabled>
                                                                    </div>
                                                                    <div class="col-sm-4 pb-1">
                                                                        <label for="CodigoIniciativa">Código Iniciativa</label>
                                                                        <input type="text" class="form-control" id="CodigoIniciativa" name="CodigoIniciativa" style="height:36px; font-size:11px;" value="@Html.Raw(@ViewBag.IdenCodigo)" placeholder="">
                                                                    </div>
                                                                    <div class="col-sm-2 pb-1">
                                                                        <label for="CodigoProyecto">Código del Proyecto</label>
                                                                        <input type="text" class="form-control" id="CodigoProyecto" name="CodigoProyecto" style="height:36px; font-size:11px;" value="@Html.Raw(@ViewBag.IdenProyecto)" maxlength="12">
                                                                        <label><code>Max. 12 Caracteres</code></label>
                                                                    </div>
                                                                    <div class="col-sm-4 pb-1">
                                                                        <label for="NombreProyectoAlias">Nombre del Proyecto(Alias)</label>
                                                                        <input type="text" class="form-control" style="height:36px; font-size:11px;text-transform:uppercase;" id="NombreProyectoAlias" name="NombreProyectoAlias" maxlength="40">
                                                                        <label><code>Max. 40 Caracteres</code></label>
                                                                    </div>
                                                                </div>
                                                            </div>
                                                        </div>



                                                        @Html.Partial("EstructuraGerenciaPersistente")

                                                        <div class="clearfix">
                                                            @{if (ViewBag.EstadoActualFlujo != 1 && ViewBag.EstadoActualFlujo != 3 && ViewBag.EstadoActualFlujo != 4)
                                                                {
                                                                    <button class="btn btn-primary" id="BotonGuardarIdentificacion" onclick="FNGuardarIdentificacion()" style="display:none;">Guardar</button>
                                                                }}&nbsp; &nbsp;
                                                            @{if (ViewBag.EstadoActualFlujo != 1 && ViewBag.EstadoActualFlujo != 3 && ViewBag.EstadoActualFlujo != 4)
                                                                {
                                                                    <button class="btn btn-warning" id="BotonActualizarIdentificacion" onclick="FNActualizarIdentificacion()">Actualizar</button>
                                                                }}
                                                        </div>
                                                    </div>

                                                </form>

                                            </div>
                                        </div>
                                    </div>

                                    <!--CATEGORIZACION-->
                                    <div class="tab-pane p-3" id="t-2" role="tabpanel" style="width:100%; margin-top:-20px;margin-bottom:0px; margin-left:0px;height:370px;">
                                        <!--Inicio Vista Parcial Categorizacion-->
                                        <div class="card offset-md-0">
                                            <div class="card-body offset-md-0">
                                                <div class="container offset-md-0">
                                                    <form name="form_categorizacion" id="form_categorizacion" action="" style="width:1680px;">
