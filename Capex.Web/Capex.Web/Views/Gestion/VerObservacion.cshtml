@{
    Layout = null;
}
<!--COMENTAR-->
<div class="modal inmodal" id="ModalObservacionRechazoAdmin1" tabindex="-1" role="dialog" aria-hidden="true">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <h6 class="modal-title">Ver Observacion Iniciativa</h6>
            </div>
            <div class="modal-body" style="margin-left:5px;width:490px; font-size:14px;">
                <div id="ContenedorObservacionRechazoAdmin1">
                    <div style="font-size:11px;">

                        <div>
                            <label for="ObservacionRechazoAdmin1">Observación</label>
                            <textarea readonly class="form-control" id="ObservacionRechazoAdmin1" maxlength="400" rows="3" style="width:450px;height:80px;text-transform:uppercase; font-size:11px;" placeholder="Ver observación.."></textarea>
                        </div>
                    </div>
                </div>

                <div class="modal-footer">
                    <button type="button" class="btn btn-white" data-dismiss="modal" onclick="FNCerrarObservacionRechazoAdmin1();">Cerrar</button>
                </div>
            </div>
        </div>
    </div>
</div>


