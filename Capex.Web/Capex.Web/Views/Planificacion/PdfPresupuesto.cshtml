@{
    Layout = null;
}
<!--PDF Presupuesto-->
<style>
    body {
        text-transform: uppercase;
        font-size: 12px;
        color: #333;
        margin-left: 100px;
    }
</style>

<div class="container" style="width: 800px;">
    <table border="0" width="100%">
        <tr>
            <td style="padding: 8px;">
                <table width="100%" border="0">
                    <tr>
                        <td>
                            <table width="100%" cellspacing="0" cellpadding="0">
                                <tr>
                                    <td width="80%"><div align="center"> <div style="font-size: 18px; font-family: Tahoma; text-align:center;">Ficha Iniciativa  @ViewBag.DESCRIPCION <br />Minera Centinela</div></td>
                                    <td width="20%"><div align="center"><img src="@Server.MapPath("~/Images/logo-centinela.jpg")" /></div></td>
                                </tr>
                            </table>
                        </td>
                    </tr>
                </table>
                <br />
                @foreach (var Idd in ViewBag.Identificacion)
                {
                    <table width="100%" border="0" style="font-size: 10px; font-family: Tahoma;">
                        <tr>
                            <td width="50%"><div align="left"><b>Nombre de Proyecto(Alias) :</b> @ViewBag.NombreProyectoAlias</div></td>
                        </tr>
                        <tr>
                            <td width="50%"><div align="left"><b>Nombre de Proyecto :</b> @ViewBag.NombreProyecto</div></td>
                        </tr>
                        <tr>
                            <td width="50%"><div align="left"><b>Código Iniciativa : </b> @ViewBag.CodigoIniciativa</div></td>
                        </tr>
                    </table>
                    <table width="100%" border="0" style="font-size: 10px; font-family: Tahoma;">
                        <tr>
                            <td width="33%"><div align="center"><b>@ViewBag.DESCRIPCION</b> @ViewBag.anioIni</div></td>
                            <td width="34%" style="color: #333"><div align="center">Fecha Rev: @ViewBag.IrFecha</div></td>
                            <!--     <td width="33%" style="color: #333"><div align="center"><b>KUS$  <b>@ViewBag.TotalAcumFinanciero<b> / 6 meses</b> </div></td> -->
                            <td width="33%" style="color: #333"><div align="center"><b>KUS$  <b>@ViewBag.TotalAcumTotPar<b> / 6 meses</b> </div></td>
                        </tr>
                        <tr valign="top">
                            <td width="100%" colspan="3">
                                <div align="center" style="color: #333; font-size:14px;">
                                    @Idd.IdenCodigo<br />
                                    @Idd.IdenNombre
                                </div>
                            </td>
                        </tr>
                        <tr>
                            <td width="33%"><div align="center" style="background-color: #DADADA;">Gerencia Cliente</div></td>
                            <td width="34%"><div align="center" style="background-color: #DADADA;">Gerencia Ejecutor</div></td>
                            <td width="33%"><div align="center" style="background-color: #DADADA;">Superintendencia Ejecutor</div></td>
                        </tr>
                        <tr>
                            <td width="33%"><div align="center" style="color: #333; font-size: 9px;">@ViewBag.GerenciaInversion</div></td>
                            <td width="34%"><div align="center" style="color: #333; font-size: 9px;">@ViewBag.GerenciaEjecucion</div></td>
                            <td width="33%"><div align="center" style="color: #333; font-size: 9px;">@ViewBag.Superintendencia</div></td>
                        </tr>
                    </table>
                }
                <table width="100%" border="0" style="font-size: 10px; font-family: Tahoma;">
                    <tr>
                        <td width="15%"><div align="center" style="background-color: #DADADA;">Estado</div></td>
                        <td width="20%"><div align="center" style="background-color: #DADADA;">Macrocategoría</div></td>
                        <td width="30%"><div align="center" style="background-color: #DADADA;">Categoría</div></td>
                        <td width="20%"><div align="center" style="background-color: #DADADA;">Clasificación SSO</div></td>
                        <td width="15%"><div align="center" style="background-color: #DADADA;">Clase</div></td>
                    </tr>
                    <tr>
                        <td width="15%"><div align="center" style="color: #333; font-size: 9px;">@ViewBag.Estado</div></td>
                        <td width="20%"><div align="center" style="color: #333; font-size: 9px;">@ViewBag.MacroCategoria</div></td>
                        <td width="30%"><div align="center" style="color: #333; font-size: 9px;">@ViewBag.Categoria</div></td>
                        <td width="20%"><div align="center" style="color: #333; font-size: 9px;">@ViewBag.ClasificacionSSO</div></td>
                        <td width="15%"><div align="center" style="color: #333; font-size: 9px;">@ViewBag.Clase</div></td>
                    </tr>
                </table>
                <table width="100%" border="0" style="font-size: 10px; font-family: Tahoma;">
                    <tr>
                        <td width="33%"><div align="center" style="background-color: #DADADA;">Nivel de Ingeniería</div></td>
                        <td width="34%"><div align="center" style="background-color: #DADADA;">Estandar de Seguridad</div></td>
                        <td width="33%"><div align="center" style="background-color: #DADADA;">Encargado de Control (SAP)</div></td>
                    </tr>
                    <tr>
                        <td width="33%"><div align="center" style="color: #333; font-size: 9px;">@ViewBag.NivelIngenieria</div></td>
                        <td width="34%"><div align="center" style="color: #333; font-size: 9px;">@ViewBag.EstandarSeguridad</div></td>
                        <td width="33%"><div align="center" style="color: #333; font-size: 9px;">@ViewBag.EncargadoControl</div></td>
                    </tr>
                </table>
                <br />
                <table width="100%" border="0" style="font-size: 10px; font-family: Tahoma;">
                    <tr>
                        <td width="100%"><div align="left" style="background-color: #DADADA;"><b>Objetivo de la Inversión</b></div></td>
                    </tr>
                    <tr height="40px" valign="top">
                        <td width="100%"><div align="left" style="color: #333; font-size: 9px;">@ViewBag.PddObjetivo</div></td>
                    </tr>
                    <tr>
                        <td width="100%"><div align="left" style="background-color: #DADADA;"><b>Descripción de la Inversión</b></div></td>
                    </tr>
                    <tr height="40px" valign="top">
                        <td width="100%"><div align="left" style="color: #333; font-size: 9px;">@ViewBag.PddJustificacion</div></td>
                    </tr>
                    <tr>
                        <td width="100%"><div align="left" style="background-color: #DADADA;"><b>Alcance de la Inversión</b></div></td>
                    </tr>
                    <tr height="40px" valign="top">
                        <td width="100%"><div align="left" style="color: #333; font-size: 9px;">@ViewBag.PddAlcance</div></td>
                    </tr>
                </table>
                <br />
                <table width="100%" border="0" style="font-size: 10px; font-family: Tahoma;">
                    <tr>
                        <td colspan="11"><div align="left" style="background-color: #DADADA;"><b>Fechas</b></div></td>
                    </tr>
                    <tr>
                        <td width="9%"><div align="center" style="background-color: #F1EFEF;">CI</div></td>
                        <td width="9%"><div align="center" style="background-color: #F1EFEF;">CA</div></td>
                        <td width="9%"><div align="center" style="background-color: #F1EFEF;">OPR/IMM</div></td>
                        <td width="9%"><div align="center" style="background-color: #F1EFEF;">VPF</div></td>
                        <td width="9%"><div align="center" style="background-color: #F1EFEF;">PE</div></td>
                        <td width="9%"><div align="center" style="background-color: #F1EFEF;">Dir PLC</div></td>
                        <td width="9%"><div align="center" style="background-color: #F1EFEF;">Dir MCEN</div></td>
                        <th width="9%"><div align="center" style="background-color: #DADADA;">Inicio</div></th>
                        <th width="9%"><div align="center" style="background-color: #DADADA;">Término</div></th>
                        <th width="9%"><div align="center" style="background-color: #DADADA;">Cierre</div></th>
                        <td width="9%"><div align="center" style="background-color: #F1EFEF;">Post- Eval.</div></td>
                    </tr>
                    <tr>
                        <td width="9%"><div align="center" style="color: #333; font-size: 9px;">@ViewBag.HCI</div></td>
                        <td width="9%"><div align="center" style="color: #333; font-size: 9px;">@ViewBag.HCA</div></td>
                        <td width="9%"><div align="center" style="color: #333; font-size: 9px;">@ViewBag.HOPR</div></td>
                        <td width="9%"><div align="center" style="color: #333; font-size: 9px;">@ViewBag.HVPF</div></td>
                        <td width="9%"><div align="center" style="color: #333; font-size: 9px;">@ViewBag.HPE</div></td>
                        <td width="9%"><div align="center" style="color: #333; font-size: 9px;">@ViewBag.HDIRPLC</div></td>
                        <td width="9%"><div align="center" style="color: #333; font-size: 9px;">@ViewBag.HDIRMCEN</div></td>
                        <td width="9%"><div align="center" style="color: #333; font-size: 9px;">@ViewBag.HINICIO</div></td>
                        <td width="9%"><div align="center" style="color: #333; font-size: 9px;">@ViewBag.HTERMINO</div></td>
                        <td width="9%"><div align="center" style="color: #333; font-size: 9px;">@ViewBag.HCIERRE</div></td>
                        <td width="9%"><div align="center" style="color: #333; font-size: 9px;">@ViewBag.HPOSTEVAL</div></td>
                    </tr>
                </table>
                <br />
                <table width="100%" border="0" style="font-size: 10px; font-family: Tahoma;">
                    <tr>
                        <!-- <td colspan="7"><div align="left" style="background-color: #DADADA;"><b>CAPEX (KUS$ <small style="color: #333;"> <b>@ViewBag.TotalAcumFinanciero<b></small>)</b></div></td>  -->
