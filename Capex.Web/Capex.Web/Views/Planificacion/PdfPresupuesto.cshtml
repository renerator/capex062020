                            <table width="100%" border="0" style="font-size: 10px; font-family: Tahoma;">
                                <tr>
                                    <td width="6%"></td>
                                    <td width="6%"><div align="center"> Per. Ant.</div></td>
                                    <td width="5%"><div align="center"> E</div></td>
                                    <td width="5%"><div align="center"> F</div></td>
                                    <td width="5%"><div align="center"> M</div></td>
                                    <td width="5%"><div align="center"> A</div></td>
                                    <td width="5%"><div align="center"> M</div></td>
                                    <td width="5%"><div align="center"> JU</div></td>
                                    <td width="5%"><div align="center"> JL</div></td>
                                    <td width="5%"><div align="center"> A</div></td>
                                    <td width="5%"><div align="center"> S</div></td>
                                    <td width="5%"><div align="center"> O</div></td>
                                    <td width="5%"><div align="center"> N</div></td>
                                    <td width="5%"><div align="center"> D</div></td>
                                    <td width="7%"><div align="center" style="background-color: #DADADA;"><b> Total @ViewBag.anioIni<b></div></td>
                                    <td width="7%"><div align="center"> Año  @ViewBag.AnioUno</div></td>
                                    <td width="7%"><div align="center"> Año @ViewBag.AnioDos</div></td>
                                    <td width="7%"><div align="center" style="background-color: #DADADA;"><b> Total<b></div></td>
                                </tr>
                                <tr>
                                    <td>Financiero</td>
                                    <td><div align="center" style="color: #333; font-size: 9px;">@ViewBag.TotalCapexTotPar</div></td>
                                    <td><div align="center" style="color: #333; font-size: 9px;">@ViewBag.EneroTotPar</div></td>
                                    <td><div align="center" style="color: #333; font-size: 9px;">@ViewBag.FebreroTotPar</div></td>
                                    <td><div align="center" style="color: #333; font-size: 9px;">@ViewBag.MarzoTotPar</div></td>
                                    <td><div align="center" style="color: #333; font-size: 9px;">@ViewBag.AbrilTotPar</div></td>
                                    <td><div align="center" style="color: #333; font-size: 9px;">@ViewBag.MayoTotPar</div></td>
                                    <td><div align="center" style="color: #333; font-size: 9px;">@ViewBag.JunioTotPar</div></td>
                                    <td><div align="center" style="color: #333; font-size: 9px;">@ViewBag.JulioTotPar</div></td>
                                    <td><div align="center" style="color: #333; font-size: 9px;">@ViewBag.AgostoTotPar</div></td>
                                    <td><div align="center" style="color: #333; font-size: 9px;">@ViewBag.SeptiembreTotPar</div></td>
                                    <td><div align="center" style="color: #333; font-size: 9px;">@ViewBag.OctubreTotPar</div></td>
                                    <td><div align="center" style="color: #333; font-size: 9px;">@ViewBag.NoviembreTotPar</div></td>
                                    <td><div align="center" style="color: #333; font-size: 9px;">@ViewBag.DiciembreTotPar</div></td>
                                    <td><div align="center" style="background-color: #DADADA; font-size: 9px;"><b>@ViewBag.TotalTotPar<b></div></td>
                                    <td><div align="center" style="color: #333; font-size: 9px;">@ViewBag.PresAnioMasUnoTotPar</div></td>
                                    <td><div align="center" style="color: #333; font-size: 9px;">@ViewBag.PresAnioMasDosTotPar</div></td>
                                    <td><div align="center" style="background-color: #DADADA; font-size: 9px;"><b>@ViewBag.TotalAcumTotPar<b></div></td>
                                </tr>
                                <tr>
                                    <td>Físico</td>
                                    <td><div align="center" style="color: #333; font-size: 9px;">@ViewBag.TotalCapexFisico</div></td>
                                    <td><div align="center" style="color: #333; font-size: 9px;">@ViewBag.EneroFisico</div></td>
                                    <td><div align="center" style="color: #333; font-size: 9px;">@ViewBag.FebreroFisico</div></td>
                                    <td><div align="center" style="color: #333; font-size: 9px;">@ViewBag.MarzoFisico</div></td>
                                    <td><div align="center" style="color: #333; font-size: 9px;">@ViewBag.AbrilFisico</div></td>
                                    <td><div align="center" style="color: #333; font-size: 9px;">@ViewBag.MayoFisico</div></td>
                                    <td><div align="center" style="color: #333; font-size: 9px;">@ViewBag.JunioFisico</div></td>
                                    <td><div align="center" style="color: #333; font-size: 9px;">@ViewBag.JulioFisico</div></td>
                                    <td><div align="center" style="color: #333; font-size: 9px;">@ViewBag.AgostoFisico</div></td>
                                    <td><div align="center" style="color: #333; font-size: 9px;">@ViewBag.SeptiembreFisico</div></td>
                                    <td><div align="center" style="color: #333; font-size: 9px;">@ViewBag.OctubreFisico</div></td>
                                    <td><div align="center" style="color: #333; font-size: 9px;">@ViewBag.NoviembreFisico</div></td>
                                    <td><div align="center" style="color: #333; font-size: 9px;">@ViewBag.DiciembreFisico</div></td>
                                    <td><div align="center" style="background-color: #DADADA; font-size: 9px;"><b>@ViewBag.TotalFisico<b></div></td>
                                    <td><div align="center" style="color: #333; font-size: 9px;">@ViewBag.PresAnioMasUnoFisico</div></td>
                                    <td><div align="center" style="color: #333; font-size: 9px;">@ViewBag.PresAnioMasDosFisico</div></td>
                                    <td><div align="center" style="background-color: #DADADA; font-size: 9px;"><b>@ViewBag.TotalAcumFisico<b></div></td>
