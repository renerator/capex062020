@{
    Layout = null;
}

<!--Inicio Vista Parcial Evaluación Economica-->
<div class="card offset-md-0">
    <div class="card-body offset-md-0">
        <!--<form name="form_eval_eco" id="form_eval_eco" action="" style="width:1680px;">-->
        <div class="container offset-md-0">


            <div class="row">
                <div class="col-xl-12 offset-md-0">
                    <div class="form-row mt-0" style="width:500px;">
                        <form name="form_eval_eco" id="form_eval_eco" action="">
                            <table class="table table-bordered mb-0">
                                <thead>
                                    <tr style='text-align:center;background-color:#5c808d;'>
                                        <th><div align="center" style="width:100px;">VAN (KUS$)</div></th>
                                        <th><div align="center" style="width:100px;">TIR (%)</div></th>
                                        <th><div align="center" style="width:100px;">IVAN</div></th>
                                        <th><div align="center" style="width:100px;">PayBack</div></th>
                                        <th><div align="center" style="width:100px;">Vida Util (Años)</div></th>
                                        <th><div align="center" style="width:100px;">TC (CLP/US$)</div></th>
                                    </tr>
                                </thead>
                                <tbody>
                                    <tr>
                                        <td><input type="text" class="form-control" style="text-align:center;text-transform:uppercase; width:100px;font-size:11px;" maxlength="8" value="0" id="EveVan" onkeyup="return FNCheckDownKey2('EveVan')" name="EveVan"></td>
                                        <td><input type="text" class="form-control" style="text-align:center;text-transform:uppercase; width:100px;font-size:11px;" maxlength="7" value="0" onkeyup="return FNCheckDownKey(event.key)" id="EveTir" name="EveTir"></td>
                                        <td><input type="text" class="form-control" style="text-align:center;text-transform:uppercase; width:100px;font-size:11px;" maxlength="8" value="0" id="EveIvan" onkeyup="return FNCheckDownKey2('EveIvan')" name="EveIvan"></td>
                                        <td><input type="text" class="form-control" style="text-align:center;text-transform:uppercase; width:100px;font-size:11px;" maxlength="8" value="0" id="EvePayBack" onkeyup="return FNCheckDownKey2('EvePayBack')" name="EvePayBack"></td>
                                        <td><input type="text" class="form-control" style="text-align:center;text-transform:uppercase; width:100px;font-size:11px;" maxlength="8" value="0" id="EveVidaUtil" onkeyup="return FNCheckDownKey2('EveVidaUtil')" name="EveVidaUtil"></td>
                                        <td><input type="text" class="form-control" style="text-align:center;text-transform:uppercase; width:100px;font-size:11px;" maxlength="8" value="0" id="EveTipoCambio" onkeyup="return FNCheckDownKey2('EveTipoCambio')" name="EveTipoCambio"></td>
                                    </tr>
                                </tbody>
                            </table>
                        </form>
                    </div>
                </div>
            </div>
            <br />
            <div class="row">
                <div class="col-md-12 offset-md-0">
                    <div class="form-row mt-12">
                        <div class="col-sm-8 pb-1">
                            <span><h7> Adjuntar Archivo de Respaldo</h7></span>
                            <br />
                            <br />
                            <!--
                            <label for="ArchivoEvalEco" style="border: 1px dashed #6995a1; height: 28px; width:400px; padding:3px 3px 3px 3px;"><span id="NombreArchivoEvalEco">Seleccione un archivo..</span></label>
                            <input id="ArchivoEvalEco" name="AdjuntarArchivosRespaldoEvalEco" style="font-size:12px;" class="inputfile inputfile-1" type="file">
                            <input type="submit" class="btn btn-success btn-sm" id="BotonAdjuntarArchivoEvalEco" value="Adjuntar" />
                            -->
                            <iframe src="~/WebForm1.aspx?type=25" scrolling="no" id="frame25" frameborder="0" height="70" width="390"></iframe>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <!--</form>-->
        <div class="clearfix">
            <button class="btn btn-primary" id="BotonGuardarEvaluacionEconomica" onclick="FNGuardarEvaluacionEconomica()">Guardar</button>
            <button class="btn btn-warning" id="BotonActualizarEvaluacionEconomica" onclick="FNActualizarEvaluacionEconomica()" style="display:none;">Actualizar</button>
        </div>
    </div>
</div>


